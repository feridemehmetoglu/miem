<div class="container">   
 <div class="row">
         <div class="col-md-6">
            <div class="row">   


                     <% if flash[:notice] %>
                     <div class="notice"><%= flash[:notice] %></div>
                      <% end %>
  <form class="form-horizontal">
  <fieldset>
    <legend> Profilinize Hoşgeldiniz Sayın, <%= @user_profile.name%></legend>
    <div class="form-group">
       <div class="col-md-2"><label>Ad:</label></div>
       <div class="col-md-9"><%= @user_profile.name%></div>
    </div>
     <div class="form-group">
       <div class="col-md-2"><label>Soyad:</label></div>
       <div class="col-md-9"><%= @user_profile.surname%></div>
    </div>
     <div class="form-group">
       <div class="col-md-2"><label>Email:</label></div>
       <div class="col-md-9"><%= @user_profile.email%></div>
    </div>
     <div class="form-group">
       <div class="col-md-2"><label>Mobile:</label></div>
       <div class="col-md-9"><%= @user_profile.mobile_phone%></div>
    </div>
     <div class="form-group">
       <div class="col-md-2"><label>Register No:</label></div>
       <div class="col-md-9"><%= @user_profile.register_no%></div>
    </div>
                
    </div>
    <div class="form-group">
              <div class="col-md-10 col-md-offset-2"> <%= link_to "Profil Düzenle",edit_user_profile_path(@user_profile), class: "btn btn-info" %></div>
             </div>
</fieldset>
</form>
           </div>
        </div>
    </div>
</div>    <!-- container -->
